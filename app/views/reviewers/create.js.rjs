msg = "#{@reviewer.person.full_name} has been added as a reviewer.  Click or search to add more."
page["#new_reviewer_#{@reviewer.person_id}"].fade_out
page["#review_#{@review.id} td.status"].html("#{@review.percent}%")
page << render(:file => "reviews/show")
page << "$('#dialog-message').remove();"
page << %|$("#new_reviewer_dialog + .ui-dialog-buttonpane button").after("<span id='dialog-message'></span>");|
page << %|$("#dialog-message").html('#{msg}');|
